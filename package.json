        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "@types/node": "^6.0.40",
        "vscode": "^1.1.0"
    },
     "dependencies": {
        "line-by-line": "^0.1.5"
  }
}