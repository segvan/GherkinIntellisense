{
    "name": "gherkinintellisense",
    "displayName": "Gherkin Intellisense",
    "description": "Gherkin IntelliSense support",
    "version": "0.0.2",
    "publisher": "Sergii Trotsenko",
    "engines": {
        "vscode": "^1.1.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
    "type": "git",
    "url": "https://github.com/segvan/GherkinIntellisense.git"
  },
    "activationEvents": [
        "onLanguage:feature"
    ],
    "main": "./out/src/extension",     
    "scripts": {
