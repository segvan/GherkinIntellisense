{
    "name": "gherkinintellisense",
    "displayName": "Gherkin Intellisense",
    "description": "Gherkin IntelliSense support",
    "version": "0.0.1",
    "publisher": "STrotsenko",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
    "type": "git",
    "url": "https://github.com/segvan/GherkinIntellisense.git"
  },
    "activationEvents": [
        "onLanguage:feature"
    ],
    "main": "./out/src/extension",     
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    },
     "dependencies": {
        "line-by-line": "0.1.5"
  }
}